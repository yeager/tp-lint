msgid ""
"tp-lint sv\n"
"tp-lint --list\n"
"tp-lint --stats sv\n"
"tp-lint --stats global --top 20\n"
"tp-lint -d coreutils sv\n"
"tp-lint --report sv --report-format html --report-output report.html\n"
msgstr ""
"tp-lint sv\n"
"tp-lint --list\n"
"tp-lint --stats sv\n"
"tp-lint --stats global --top 20\n"
"tp-lint -d coreutils sv\n"
"tp-lint --report sv --report-format html --report-output rapport.html\n"

