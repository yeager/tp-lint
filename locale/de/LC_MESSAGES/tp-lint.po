msgid "Error fetching team index: {error}"
msgstr "Fehler beim Abrufen des Teamindexes: {error}"

msgid "Language '{lang}' not found on Translation Project"
msgstr "Sprache '{lang}' wurde im Translation Project nicht gefunden"

msgid "Error fetching team page: {error}"
msgstr "Fehler beim Abrufen der Teamseite: {error}"

msgid "Error downloading {url}: {error}"
msgstr "Fehler beim Herunterladen von {url}: {error}"

msgid "Error: l10n-lint not found. Please install l10n-lint first."
msgstr "Fehler: l10n-lint nicht gefunden. Bitte installieren Sie zuerst l10n-lint."

msgid "Lint PO files from the Translation Project (translationproject.org)"
msgstr "PO-Dateien vom Translation Project (translationproject.org) pruefen"

msgid "Example: tp-lint sv"
msgstr "Beispiel: tp-lint sv"

msgid "Language code to lint (e.g., sv, de, fr)"
msgstr "Sprachcode zum Pruefen (z.B. sv, de, fr)"

msgid "List all available languages"
msgstr "Alle verfuegbaren Sprachen auflisten"

msgid "Output format (default: text)"
msgstr "Ausgabeformat (Standard: text)"

msgid "Treat warnings as errors"
msgstr "Warnungen als Fehler behandeln"

msgid "Keep downloaded PO files (don't delete temp directory)"
msgstr "Heruntergeladene PO-Dateien behalten (temporaeres Verzeichnis nicht loeschen)"

msgid "Output directory for downloaded PO files (implies --keep)"
msgstr "Ausgabeverzeichnis fuer heruntergeladene PO-Dateien (impliziert --keep)"

msgid "Only lint specific package(s) (can be repeated)"
msgstr "Nur bestimmte Pakete pruefen (kann wiederholt werden)"

msgid "Fetching languages from Translation Project..."
msgstr "Sprachen vom Translation Project werden abgerufen..."

msgid "Could not fetch language list"
msgstr "Sprachliste konnte nicht abgerufen werden"

msgid "Available languages ({count}):"
msgstr "Verfuegbare Sprachen ({count}):"

msgid "Fetching PO files for '{lang}'..."
msgstr "PO-Dateien fuer '{lang}' werden abgerufen..."

msgid "No PO files found for language '{lang}'"
msgstr "Keine PO-Dateien fuer Sprache '{lang}' gefunden"

msgid "No matching packages found"
msgstr "Keine passenden Pakete gefunden"

msgid "Found {count} PO files"
msgstr "{count} PO-Dateien gefunden"

msgid "Downloading PO files..."
msgstr "PO-Dateien werden heruntergeladen..."

msgid "Downloaded {count} files"
msgstr "{count} Dateien heruntergeladen"

msgid "No files to lint"
msgstr "Keine Dateien zum Pruefen"

msgid "Running l10n-lint..."
msgstr "l10n-lint wird ausgefuehrt..."

msgid "PO files saved to: {path}"
msgstr "PO-Dateien gespeichert unter: {path}"
